{
  "name": "AventCode",
  "version": "1.0.0",
  "description": "Michael Kramer's Avent Code",
  "main": "index.js",
  "scripts": {
    "test": "nf run -- NODE_ENV=test ava",
    "puglint": "pug-lint server/web/views",
    "flow": "flow check",
    "lint": "NODE_PATH=. eslint --ext .js .",
    "migrate": "nf run -- mariner migrate up",
    "rollback": "nf run -- mariner migrate down",
    "web": "nf run -- nodemon index.js web",
    "workers": "nf run -- node index.js workers",
    "crons": "nf run -- node index.js cron"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:michaelkramer/aventcode.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.14.0",
    "babel-register": "^6.9.0",
    "bluebird": "^3.5.0",
    "commander": "^2.9.0",
    "distraught": "^1.2.1",
    "glob": "^7.1.2",
    "lodash": "^4.17.1",
    "mariner": "^1.0.2",
    "moment": "^2.12.0",
    "pg": "^6.4.2",
    "pug": "^2.0.0-rc.3",
    "redis": "^2.6.3"
  },
  "devDependencies": {
    "ava": "^0.18.1",
    "babel-eslint": "^7.2.3",
    "cheerio": "^1.0.0-rc.2",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-es5": "^0.5.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "flow-bin": "^0.52.0",
    "foreman": "^2.0.0",
    "mock-knex": "^0.3.9",
    "nodemon": "^1.11.0",
    "pug-lint": "^2.4.0"
  },
  "ava": {
    "files": [
      "test/templates/*-test.js",
      "test/server/*-test.js"
    ],
    "concurrency": 3,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit",
    "source": "**/*.{js,pug}"
  },
  "engines": {
    "node": "^8.*"
  }
}
